// call of a nonsense procedure

define N 6
define M (1<<N) - 2
//define DEFAULT_INT_BITS N

init main;

error:	goto error;
ok:	goto ok;

module void nonsense()
{
	int x(N);
	x = M;
	do
	:: (x>0)  -> x = x-1;
	:: else   ->  return;
	od;
}

module void start()
{
	int a(1); 
	a = 1;
	if
	:: true ->  nonsense();
	:: true -> skip;
	fi;

	if
	:: a == 0 -> goto error;
	:: else	 -> goto ok;
	fi;

	return;
}

main: 	start();
halt: goto halt;

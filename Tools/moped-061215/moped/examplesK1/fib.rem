define DEFAULT_INT_BITS 5

init main;

module int fib(int n) {
	
	int n_1, n_2;
	
	if
	:: n==0	->	return 0;
	:: n==1 ->	return 1;
	:: else	->	n_1 = fib(n-1);
			n_2 = fib(n-2);
			return n_1+n_2;
	fi;
}

module void main() {
	
	int r;
	r = fib(8);
	if
	:: r==21	->	ok: goto ok;
	:: else		->	error: goto error;
	fi;
}

define N 4
define DEFAULT_INT_BITS N
define M (1<<N) - 2

init main;

error:	goto error;
ok:	goto ok;

module int inc(int a)
{
	int b;
	
	do
	:: true -> skip (b'>0);
		   if
		   :: (b == a+1) -> return b;
		   :: else -> skip;
		   fi;
	od;
}
	

module void main()
{
	int x, y;
	y = inc(x);
	if
	:: (y == x+1)	-> goto ok;
	:: else		-> goto error;
	fi;
}
	
